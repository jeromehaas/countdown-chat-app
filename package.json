{
  "name": "new-year-countdown-app",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "concurrently \"npm run styles\" \"npm run scripts\" \"npm run watch\" \"nodemon app.js\"",
    "styles": "npm run styles:build & npm run styles:prefix & npm run styles:compress",
    "styles:prefix": "postcss public/css/main.css --use-autoprefixer --map=false --output=css/main.css --watch",
    "styles:build": "sass src/scss/central.scss public/css/main.css --watch",
    "styles:compress": "sass public/css/main.css public/css/main.css --style=compressed --watch",
    "scripts": "npm run scripts:build",
    "scripts:build": "webpack --watch",
    "watch": "browser-sync start --proxy localhost:3005 --files=**/* --no-open --ignore=node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.19.0",
    "@babel/plugin-transform-object-assign": "^7.18.6",
    "@babel/preset-env": "^7.19.0",
    "autoprefixer": "^10.4.8",
    "babel-loader": "^8.2.5",
    "browser-sync": "^2.27.10",
    "concurrently": "^7.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-handlebars": "^6.0.6",
    "gsap": "^3.11.1",
    "http": "^0.0.1-security",
    "mongoose": "^6.6.1",
    "nodemon": "^2.0.20",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "qrcode": "^1.5.1",
    "sass": "^1.55.0",
    "socket.io": "^4.5.2",
    "socket.io-client": "^4.5.2",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
